## Code Jam {{ year.name }}

<table>
    {% for round in year.rounds %}
        {% for problem in round.problems %}
    <tr>
            {% if loop.first %}
        <th rowspan="{{ round.problems|count }}"><a href="{{ round.url }}">{{ round.name }}</a></th>
            {% endif %}
        <td><a href="{{ problem.url }}">{{ problem.name }}</a></td>
        <td>
            <ul>
            {% for test_set in problem.test_sets %}
                <li>[ ] {{ test_set }}</li>
            {% endfor %}
            </ul>
        </td>
    </tr>
        {% endfor %}
    {% endfor %}
</table>
